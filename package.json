{
	"name": "express-boilerplate",
	"version": "1.0.0",
	"description": "A simple boilerplate for creating Express applications.",
	"author": {
		"name": "Miloš Milanović",
		"email": "mmilanovic016@gmail.com"
	},
	"license": "LGPL-3.0-or-later",
	"scripts": {
		"clean": "rm -rf dist/*",
		"format": "prettier --write src/**/*.{js,jsx,css,json} *.{js,json}",
		"build:dev": "webpack --progress",
		"build:prod": "webpack --env.prod --progress",
		"build:stats": "webpack --env.prod --progress --profile --json > stats.json",
		"start:dev": "nodemon dist/server.js",
		"start:prod": "NODE_ENV=production node dist/server.js"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mmilanovic4/express-boilerplate.git"
	},
	"devDependencies": {
		"@babel/core": "^7.7.5",
		"@babel/preset-env": "^7.7.6",
		"babel-loader": "^8.0.6",
		"copy-webpack-plugin": "^5.1.1",
		"nodemon": "^2.0.2",
		"prettier": "1.19.1",
		"webpack": "^4.41.6",
		"webpack-cli": "^3.3.11",
		"webpack-node-externals": "^1.7.2"
	},
	"dependencies": {
		"cookie-parser": "^1.4.4",
		"core-js": "^3.4.8",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"pug": "^2.0.4",
		"regenerator-runtime": "^0.13.3"
	}
}
